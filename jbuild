(jbuild_version 1)

(executables
    (
        (names (csv_query QueryParserTest))
        (libraries (angstrom core ounit qcheck str))
        (ocamlc_flags (-g))
        (preprocess (pps (ppx_sexp_conv)))
    )
)

(alias
 ((name    runtest)
  (deps    (QueryParserTest.bc))
  (action  (run ${<}))))